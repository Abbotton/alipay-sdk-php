{
    "name": "wi1dcard/alipay-sdk",
    "description": "支付宝（蚂蚁金服）开放平台第三方 PHP SDK，基于官方 3.3.0 版本。",
    "type": "library",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": ">=4.8",
        "vlucas/phpdotenv": "^2.5"
    },
    "authors": [
        {
            "name": "wi1dcard",
            "email": "wi1dcard@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alipay\\": "aop/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alipay\\Test\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/phpcs aop -p --colors --standard='PSR1,PSR2,PSR12' -n",
        "fix-cs": "vendor/bin/phpcbf aop -p --colors --standard='PSR1,PSR2,PSR12,Zend'",
        "phpunit": "vendor/bin/phpunit"
    }
}
