{
    "name": "wi1dcard/alipay-sdk",
    "description": "支付宝（蚂蚁金服）开放平台第三方 PHP SDK，基于官方 3.3.1 版本。",
    "type": "library",
    "require": {
        "php": ">=5.4",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "vlucas/phpdotenv": "^2.5"
    },
    "authors": [
        {
            "name": "wi1dcard",
            "email": "wi1dcard.cn@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alipay\\": "aop/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alipay\\Test\\": "tests"
        }
    }
}
