{
  "name": "t3rryw1/alipay-sdk",
  "description": "支付宝（蚂蚁金服）开放平台第三方 PHP SDK，基于官方 3.3.2 版本。",
  "type": "library",
  "require": {
    "php": ">=5.4",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8",
    "vlucas/phpdotenv": "^2.5"
  },
  "authors": [
    {
      "name": "t3rryw1",
      "email": "zhichangterry@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Alipay\\": "aop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alipay\\Test\\": "tests"
    }
  }
}
